{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "indent": "off",
    "max-len": "off",
    "operator-linebreak": "off"
  },
  "globals": {
    "IS_DEV": true
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended",
    "google"
  ]
}
